{
  "name": "open-twa",
  "version": "0.0.1",
  "description": "Open Builders TWA React UI components",
  "type": "module",
  "main": "./dist/open-twa.es.js",
  "module": "./dist/open-twa.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/open-twa.es.js",
      "require": "./dist/open-twa.umd.cjs",
      "types": "./dist/main.d.ts"
    },
    "./styles.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "components",
    "react",
    "react-component",
    "ui",
    "design-system",
    "telegram-sdk",
    "twa"
  ],
  "author": "Open Builders",
  "repository": "https://github.com/open-builders/open-twa",
  "bugs": {
    "url": "https://github.com/open-builders/open-twa/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^5.0.0",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0"
  }
}
